      x_0.write_en = 1'd1;
      let0[done] = x_0.done;
      slice0.in = const0.out;
    }
    group let1 {
      tmp_0.in = foo0.out;
      tmp_0.write_en = 1'd1;
      let1[done] = tmp_0.done;
    }
  }
  control {
    seq {
      let0;
      invoke foo0(x=x_0.out)();
      let1;
    }
  }
