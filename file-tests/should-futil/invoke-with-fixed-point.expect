import "primitives/std.lib";
component foo(x: 8) -> (out: 8) {
  cells {
    slice1 = std_slice(8,8);
    y_0 = std_reg(8);
  }
  wires {
    group let2<"static"=1> {
      y_0.in = slice1.out;
      y_0.write_en = 1'd1;
      let2[done] = y_0.done;
      slice1.in = x;
    }
    out = y_0.out;
  }
  control {
    let2;
  }
}
