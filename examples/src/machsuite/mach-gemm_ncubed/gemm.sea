def gemm( m1: float[64][64 bank 64], m2: float[64 bank 64][64], prod: float[64][64] ){

    for (let i = 0..64) unroll 1 {
        for (let j = 0..64) unroll 1 {
            let sum = 0.0;
            for( let k = 0..64) unroll 64 {
                let mult = m1[i][k] * m2[k][j];
            } combine {
                sum += mult;
            };
            prod[i][j] := sum;
        }
    }

}
